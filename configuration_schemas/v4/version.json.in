    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "catalog_schema_version": {"type": "integer" },
        "commit_id": {"type": "string", "pattern": "^[0-9a-f]{40}$"},
        "configuration_schema_version": {"type": "integer" },
