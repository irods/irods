{
    "$id": "https://irods.org/configuration_schemas/v@IRODS_CONFIGURATION_SCHEMA_VERSION@/server_config.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "advanced_settings": {
            "type": "object",
            "properties": {
                "default_number_of_transfer_threads": {
                    "type": "integer",
                    "minimum": 0
                },
                "default_temporary_password_lifetime_in_seconds": {
                    "type": "integer",
                    "minimum": 0
                },
                "delay_rule_executors": {
                    "type": "array",
                    "items": {"type": "string"},
                    "uniqueItems": true
                },
                "delay_server_sleep_time_in_seconds": {
                    "type": "integer",
                    "minimum": 0
                },
                "dns_cache": {
                    "type": "object",
                    "properties": {
                        "cache_clearer_sleep_time_in_seconds": {
                            "type": "integer",
                            "minimum": 0
                        },
                        "eviction_age_in_seconds": {
                            "type": "integer",
                            "minimum": 0
                        },
                        "shared_memory_size_in_bytes": {
                            "type": "integer",
                            "minimum": 1
                        }
                    },
                    "required": [
                        "cache_clearer_sleep_time_in_seconds",
                        "eviction_age_in_seconds",
                        "shared_memory_size_in_bytes"
                    ]
                },
                "hostname_cache": {
                    "type": "object",
                    "properties": {
                        "cache_clearer_sleep_time_in_seconds": {
                            "type": "integer",
                            "minimum": 0
                        },
                        "eviction_age_in_seconds": {
                            "type": "integer",
                            "minimum": 0
                        },
                        "shared_memory_size_in_bytes": {
                            "type": "integer",
                            "minimum": 1
                        }
                    },
                    "required": [
                        "cache_clearer_sleep_time_in_seconds",
                        "eviction_age_in_seconds",
                        "shared_memory_size_in_bytes"
                    ]
                },
                "maximum_size_for_single_buffer_in_megabytes": {
                    "type": "integer",
                    "minimum": 1
                },
                "maximum_size_of_delay_queue_in_bytes": {
                    "type": "integer",
                    "minimum": 0
                },
                "maximum_temporary_password_lifetime_in_seconds": {
                    "type": "integer",
                    "minimum": 0
                },
                "migrate_delay_server_sleep_time_in_seconds": {
                    "type": "integer",
                    "minimum": 0
                },
                "number_of_concurrent_delay_rule_executors": {
                    "type": "integer",
                    "minimum": 0
                },
                "stacktrace_file_processor_sleep_time_in_seconds": {
                    "type": "integer",
                    "minimum": 0
                },
                "transfer_buffer_size_for_parallel_transfer_in_megabytes": {
                    "type": "integer",
                    "minimum": 1
                },
                "transfer_chunk_size_for_parallel_transfer_in_megabytes": {
                    "type": "integer",
                    "minimum": 1
                }
            },
            "required": [
                "default_number_of_transfer_threads",
                "default_temporary_password_lifetime_in_seconds",
                "delay_rule_executors",
                "delay_server_sleep_time_in_seconds",
                "dns_cache",
                "hostname_cache",
                "maximum_size_for_single_buffer_in_megabytes",
                "maximum_size_of_delay_queue_in_bytes",
                "maximum_temporary_password_lifetime_in_seconds",
                "migrate_delay_server_sleep_time_in_seconds",
                "number_of_concurrent_delay_rule_executors",
                "stacktrace_file_processor_sleep_time_in_seconds",
                "transfer_buffer_size_for_parallel_transfer_in_megabytes",
                "transfer_chunk_size_for_parallel_transfer_in_megabytes"
            ]
        },
        "catalog_provider_hosts": {
            "type": "array",
            "items": {"type": "string"},
            "minItems": 1,
            "uniqueItems": true
        },
        "catalog_service_role": {"enum": ["consumer", "provider"]},
        "client_api_allowlist_policy": {"enum": ["disabled", "enforce"]},
        "client_server_policy": {
            "enum": [
                "CS_NEG_REFUSE",
                "CS_NEG_DONT_CARE",
                "CS_NEG_REQUIRE"
            ]
        },
        "connection_pool_refresh_time_in_seconds": {
            "type": "integer",
            "minimum": 0
        },
        "controlled_user_connection_list": {
            "type": "object",
            "properties": {
                "control_type": {"enum": ["denylist", "allowlist"]},
                "users": {
                    "type": "array",
                    "items": {"type": "string"},
                    "uniqueItems": true
                }
            },
            "required": [
                "control_type",
                "users"
            ]
        },
        "default_dir_mode": {
            "type": "string",
            "minLength": 4,
            "maxLength": 4
        },
        "default_file_mode": {
            "type": "string",
            "minLength": 4,
            "maxLength": 4
        },
        "default_hash_scheme": {
            "type": "string",
            "maxLength": 63
        },
        "default_resource_name": {
            "type": "string",
            "maxLength": 63
        },
        "encryption": {
            "type": "object",
            "properties": {
                "algorithm": {
                    "type": "string",
                    "maxLength": 127
                },
                "key_size": {
                    "type": "integer",
                    "minimum": 32
                },
                "num_hash_rounds": {
                    "type": "integer",
                    "minimum": 16
                },
                "salt_size": {
                    "type": "integer",
                    "minimum": 8
                }
            },
            "required": [
                "algorithm",
                "key_size",
                "num_hash_rounds",
                "salt_size"
            ]
        },
        "environment_variables": {
            "type": "object",
            "additionalProperties": {"type": "string"}
        },
        "federation": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "catalog_provider_hosts": {
                        "type": "array",
                        "items": {"type": "string"},
                        "minItems": 1,
                        "uniqueItems": true
                    },
                    "negotiation_key": {
                        "type": "string",
                        "pattern": "^[A-Za-z0-9_]+$",
                        "minLength": 32,
                        "maxLength": 32
                    },
                    "zone_key": {
                        "type": "string",
                        "pattern": "^[A-Za-z0-9_]+$",
                        "maxLength": 49
                    },
                    "zone_name": {
                        "type": "string",
                        "pattern": "^[A-Za-z0-9_\\.]+$",
                        "maxLength": 63
                    },
                    "zone_port": {
                        "type": "integer",
                        "minimum": 0
                    }
                },
                "required": [
                    "catalog_provider_hosts",
                    "negotiation_key",
                    "zone_key",
                    "zone_name"
                ]
            }
        },
        "graceful_shutdown_timeout_in_seconds": {
            "type": "integer",
            "minimum": 0
        },
        "host": {
            "type": "string",
            "minLength": 1,
            "maxLength": 253
        },
        "host_access_control": {
            "$ref": "host_access_control.json"
        },
        "host_resolution": {
            "$ref": "host_resolution.json"
        },
        "log_level": {
            "type": "object",
            "items": {
                "properties": {
                    "agent": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "agent_factory": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "api": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "authentication": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "database": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "delay_server": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "legacy": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "microservice": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "network": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "resource": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "rule_engine": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1},
                    "server": {"enum": ["trace", "debug", "info", "warn", "error", "critical"], "maxItems": 1}
                }
            }
        },
        "match_hash_policy": {"type": "string"},
        "maximum_connections": {
            "type": "integer",
            "minimum": 1
        },
        "negotiation_key": {
            "type": "string",
            "pattern": "^[A-Za-z0-9_]+$",
            "minLength": 32,
            "maxLength": 32
        },
        "plugin_configuration": {
            "type": "object",
            "properties": {
                "authentication": {
                    "type": "object",
                    "properties": {}
                },
                "database": {
                    "type": "object",
                    "propertyNames": {
                        "pattern": "^[^\\s]+$"
                    },
                    "patternProperties": {
                        "^[^\\s]+$": {
                            "$ref": "database_config.json"
                        }
                    },
                    "maxProperties" : 1,
                    "minProperties" : 1
                },
                "network": {
                    "type": "object",
                    "properties": {}
                },
                "resource": {
                    "type": "object",
                    "properties": {}
                },
                "rule_engines": {
                    "type": "array",
                    "items": {
                        "$ref": "rule_engine.json"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "authentication",
                "network",
                "resource",
                "rule_engines"
            ]
        },
        "plugin_directory": {
            "type": "string",
            "maxLength": 1087
        },
        "schema_name": {"type": "string"},
        "schema_version": {"type": "string"},
        "server_port_range_end": {"type": "integer"},
        "server_port_range_start": {"type": "integer"},
        "tcp_keepalive_intvl_in_seconds": {
            "type": "integer",
            "minimum": -1
        },
        "tcp_keepalive_probes": {
            "type": "integer",
            "minimum": -1
        },
        "tcp_keepalive_time_in_seconds": {
            "type": "integer",
            "minimum": -1
        },
        "tls_server": {
            "type": "object",
            "properties": {
                "certificate_chain_file": {"type": "string"},
                "certificate_key_file": {"type": "string"},
                "dh_params_file": {"type": "string"}
            },
            "required": [
                "certificate_chain_file",
                "certificate_key_file",
                "dh_params_file"
            ]
        },
        "tls_client": {
            "type": "object",
            "properties": {
                "ca_certificate_file": {"type": "string"},
                "ca_certificate_path": {"type": "string"},
                "verify_server": {
                    "enum": [
                        "none",
                        "hostname",
                        "cert"
                    ]
                }
            },
            "required": [
                "verify_server"
            ]
        },
        "zone_auth_scheme" : {
            "type": "string",
            "maxLength": 63
        },
        "zone_key": {
            "type": "string",
            "pattern": "^[A-Za-z0-9_]+$",
            "maxLength": 49
        },
        "zone_name": {
            "type": "string",
            "pattern": "^[A-Za-z0-9_\\.]+$",
            "maxLength": 63
        },
        "zone_port": {
            "type": "integer",
            "minimum": 0
        },
        "zone_user": {
            "type": "string",
            "maxLength": 63
        }
    },
    "required": [
        "advanced_settings",
        "catalog_provider_hosts",
        "catalog_service_role",
        "client_api_allowlist_policy",
        "client_server_policy",
        "connection_pool_refresh_time_in_seconds",
        "controlled_user_connection_list",
        "default_dir_mode",
        "default_file_mode",
        "default_hash_scheme",
        "encryption",
        "environment_variables",
        "federation",
        "graceful_shutdown_timeout_in_seconds",
        "host",
        "host_access_control",
        "host_resolution",
        "log_level",
        "match_hash_policy",
        "negotiation_key",
        "plugin_configuration",
        "rule_engine_namespaces",
        "schema_name",
        "schema_version",
        "server_port_range_end",
        "server_port_range_start",
        "zone_auth_scheme",
        "zone_key",
        "zone_name",
        "zone_port",
        "zone_user"
    ]
}
